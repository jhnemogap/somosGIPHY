{
  "name": "somogiphy",
  "version": "0.1.0",
  "description": "This application is a ...",
  "source": "src/index.html",
  "scripts": {
    "clean": "rm -rf ./dist ./.parcel-cache",
    "dev": "npm run clean && parcel ./src/index.html --port 3000 --open",
    "buildLocal": "npm run clean && parcel build ./src/index.html --no-source-maps",
    "build": "parcel build ./src/index.html",
    "start": "node server.js",
    "heroku-postbuild": "parcel build ./src/index.html",
    "lint": "eslint '{*.js,src/**/*.{js,jsx,ts,tsx}}'",
    "lint-fix": "eslint --fix '{*.js,src/**/*.{js,jsx,ts,tsx}}'",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx,ts,tsx}}'"
  },
  "keywords": [],
  "author": "Jorge Humberto Nemoga Pinzon",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-thunk": "^1.0.0",
    "redux": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged",
      "pre-applypatch": "lint-staged",
      "prepare-commit-msg": "lint-stage",
      "commit-msg": "lint-stage"
    }
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx, md, css}": [
      "npm run format",
      "npm run lint-fix"
    ]
  },
  "engines": {
    "node": "14.5.x"
  },
  "targets": {
    "app": {
      "engines": {
        "browsers": "> 5%"
      }
    }
  }
}
